{
	"apps": {
		"http": {
			"servers": {
				"srv0": {
					"experimental_http3": true,
					"http_port": 8080,
					"https_port": 8443
				},
				"srv1": {
					"listen": [":443"],
					"routes": [
						{
							"handle": [
								{
									"handler": "subroute",
									"routes": [
										{
											"handle": [
												{
													"handler": "reverse_proxy",
													"health_checks": {
														"active": {
															"expect_status": 2,
															"path": "/health"
														}
													},
													"transport": {
														"protocol": "http",
														"read_buffer_size": 4096
													},
													"upstreams": [
														{
															"dial": "127.0.0.1:3000"
														}
													]
												}
											],
											"match": [
												{
													"path": ["/"]
												}
											]
										}
									]
								}
							],
							"match": [
								{
									"host": ["discoin.zws.im"]
								}
							]
						}
					]
				}
			}
		}
	}
}
