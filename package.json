	"description": "Rewritten server-side API for Discoin",
	"devDependencies": {
		"@types/common-tags": "^1.8.0",
		"@types/express": "^4.17.2",
		"@types/node": "^13.1.2",
		"@types/passport-http-bearer": "^1.0.34",
		"@typescript-eslint/eslint-plugin": "^2.14.0",
		"@typescript-eslint/parser": "^2.14.0",
		"eslint-config-xo-typescript": "^0.24.1",
		"prettier": "^1.19.1",
		"ts-node": "^8.5.4",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.7.4",
		"xo": "^0.25.3"
	},
	"engines": {
		"node": "12"
	},
	"license": "Apache-2.0",
	"name": "discoin-server-rewrite",
	"private": true,
	"scripts": {
		"build": "nest build",
		"format": "prettier --ignore-path .gitignore --check \"./**/*.{{j,t}{s,sx},flow,{sc,c,le}ss,json,m{d,dx},{y,ya}ml}\"",
		"lint": "xo",
		"prebuild": "rimraf tsc_output",
		"start": "nest start",
		"start:debug": "nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node tsc_output/main"
	},
	"version": "3.0.0",
	"xo": {
		"extends": "xo-typescript",
		"extensions": [
			"ts"
		],
		"overrides": [
			{
				"files": "**/*.service.ts",
				"rules": {
					"@typescript-eslint/no-useless-constructor": "off"
				}
			}
		],
		"prettier": true,
		"rules": {
			"import/extensions": "off",
			"new-cap": "off"
		}
	}
}
