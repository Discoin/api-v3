		"@nestjs/swagger": "^4.1.2",
		"@nestjs/terminus": "^6.5.5",
		"@nestjs/typeorm": "^6.2.0",
		"@nestjsx/crud": "^4.4.1",
		"@nestjsx/crud-typeorm": "^4.4.1",
		"@sentry/node": "5.10.2",
		"@types/ws": "^6.0.4",
		"class-transformer": "^0.2.3",
		"class-validator": "^0.11.0",
		"common-tags": "^1.8.0",
		"discord.js": "discordjs/discord.js",
		"dotenv": "^8.2.0",
		"passport": "^0.4.0",
		"passport-http-bearer": "^1.0.1",
