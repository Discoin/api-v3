		"@nestjs/common": "6.11.11",
		"@nestjs/core": "6.11.11",
		"@nestjs/passport": "6.2.0",
		"@nestjs/platform-express": "6.11.11",
		"@nestjs/swagger": "4.3.2",
		"@nestjs/terminus": "6.5.6",
		"@nestjs/typeorm": "6.3.3",
		"@nestjsx/crud": "4.4.1",
		"@nestjsx/crud-typeorm": "4.4.1",
		"@sentry/node": "5.13.2",
		"@types/ws": "7.2.2",
		"class-transformer": "0.2.3",
		"class-validator": "0.11.0",
		"common-tags": "1.8.0",
		"discord.js": "discordjs/discord.js",
		"dotenv": "8.2.0",
		"influx": "5.5.1",
		"passport": "0.4.1",
		"passport-http-bearer": "1.0.1",
		"pg": "7.18.2",
		"reflect-metadata": "0.1.13",
		"rimraf": "3.0.2",
		"rxjs": "6.5.4",
		"sqreen": "1.38.4",
		"swagger-ui-express": "4.1.2",
		"typeorm": "0.2.25"
	},
	"description": "Rewritten server-side API for Discoin",
	"devDependencies": {
		"@types/common-tags": "1.8.0",
		"@types/express": "4.17.3",
		"@types/node": "12.12.38",
		"@types/passport-http-bearer": "1.0.34",
		"prettier": "1.19.1",
		"ts-node": "8.6.2",
		"tsconfig-paths": "3.9.0",
		"typescript": "3.8.3",
		"xo": "0.28.0"
	},
	"engines": {
		"node": "12"
	},
	"license": "Apache-2.0",
	"name": "discoin-server-rewrite",
	"private": true,
	"scripts": {
		"build": "nest build",
		"format": "prettier --ignore-path .gitignore --check \"./**/*.{{j,t}{s,sx},flow,{sc,c,le}ss,json,m{d,dx},{y,ya}ml}\"",
		"lint": "xo",
		"prebuild": "rimraf tsc_output",
		"start": "nest start",
		"start:debug": "nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node tsc_output/main"
	},
	"version": "3.0.0",
	"xo": {
		"overrides": [
			{
				"files": "**/*.service.ts",
				"rules": {
					"@typescript-eslint/no-useless-constructor": "off"
				}
			}
		],
		"prettier": true,
		"rules": {
			"import/extensions": "off",
			"new-cap": "off"
		}
	}
}
